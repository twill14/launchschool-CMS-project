  <% if session[:failure]%>
    <p id="failure"><%= session.delete(:failure)%></p>
  <% end %>

  <%if session[:success] %>
   <p id="update"><%= session.delete(:success)%></p>
  <% end %>

  <form method="post" action="/users/signin">
    <div>
      Username:
      <input id="username" name="username"><br>
      Password:
      <input id="password" name="password"><br>
      <button type="submit">Sign In</button>
    </div>
  </form>